{
  "name": "vscode-temme",
  "private": true,
  "license": "MIT",
  "displayName": "Temme",
  "description": "Temme selector support for VSCode.",
  "version": "0.8.3",
  "publisher": "shinima",
  "repository": "https://github.com/shinima/temme.git",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:grammar": "supervisor --no-restart-on exit --watch temme.tmLanguage.yaml -e yaml -- compile.js",
    "start:ext": "webpack --watch --mode=development",
    "build": "npm-run-all --parallel build:*",
    "build:grammar": "node compile.js",
    "build:ext": "webpack --mode=production"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:temme",
    "onCommand:temme.runSelector",
    "onCommand:temme.startWatch",
    "onCommand:temme.stop"
  ],
  "contributes": {
    "languages": [
      {
        "id": "temme",
        "aliases": [
          "temme",
          "temme"
        ],
        "extensions": [
          ".temme"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "temme",
        "scopeName": "source.temme",
        "path": "./dist/temme.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "temme.runSelector",
        "category": "Temme",
        "title": "Run temme selector"
      },
      {
        "command": "temme.startWatch",
        "category": "Temme",
        "title": "Start watching"
      },
      {
        "command": "temme.stop",
        "category": "Temme",
        "title": "Stop watching"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^13.1.4",
    "js-yaml": "^3.13.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "supervisor": "^0.12.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "vscode": "1.1.36",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@types/node-fetch": "^2.3.7",
    "node-fetch": "^2.6.0"
  }
}
