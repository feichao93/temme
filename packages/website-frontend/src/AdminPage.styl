.admin-page {
  padding-top 50px

  .fg1 {
    flex-grow 1
  }

  .user-list {
    list-style none
    padding 0

    > li {
      display flex
      height 160px
      padding 8px 0
      border-bottom 1px solid #ccc

      .user-info {
        display flex
        flex-flow column
        align-items center
      }
    }

    .avatar {
      height 88px
    }
    .username {
      margin-top 8px
    }
  }

  .project-list-wrapper {
    display flex
    flex-flow column
    margin-left 16px
  }

  .project-list {
    list-style none
    padding 0

    display flex
    flex-flow column
    flex-wrap wrap

    > li {
      margin-top 2px
      margin-right 20px
    }
  }
}
