{
  "name": "temme",
  "version": "0.5.1",
  "description": "Concise and convenient jQuery-like selector for node crawlers.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "https://github.com/shinima/temme.git",
  "homepage": "https://temme.js.org",
  "author": "Shi Feichao <842351815@qq.com> (shinima.pw)",
  "license": "MIT",
  "bin": "./bin/temme.js",
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "webpack",
    "build-playground": "webpack --config webpack.playground.config.ts --env.production && cp -r playground/public/* playground-build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "@types/cheerio": "^0.22.1",
    "@types/commander": "^2.11.0",
    "@types/invariant": "^2.2.29",
    "cheerio": "^1.0.0-rc.1",
    "commander": "^2.12.2",
    "invariant": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.31",
    "@types/pegjs": "^0.10.0",
    "@types/webpack": "^3.0.12",
    "awesome-typescript-loader": "^3.2.3",
    "babel-minify-webpack-plugin": "^0.2.0",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "^1.7.1",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.4",
    "pretty": "^2.0.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^4.0.0",
    "typescript": "^2.4.1",
    "webpack": "^3.6.0"
  }
}
