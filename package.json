{
  "name": "temme",
  "version": "0.6.1",
  "description": "Concise and convenient jQuery-like selector for node crawlers.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "https://github.com/shinima/temme.git",
  "homepage": "https://temme.js.org",
  "author": "Shi Feichao <842351815@qq.com> (shinima.pw)",
  "license": "MIT",
  "bin": "./bin/temme.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "LICENSE",
    "readme.md",
    "bin/",
    "build/"
  ],
  "scripts": {
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "webpack",
    "build-playground": "webpack --config webpack.playground.config.ts --mode production && cp -r playground/public/* playground-build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/invariant": "^2.2.29",
    "cheerio": "^1.0.0-rc.1",
    "commander": "^2.15.0",
    "invariant": "^2.2.3"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/pegjs": "^0.10.0",
    "@types/webpack": "^4.1.0",
    "awesome-typescript-loader": "^4.0.1",
    "coveralls": "^3.0.0",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.4.2",
    "lodash.debounce": "^4.0.8",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.4",
    "pretty": "^2.0.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11"
  }
}
